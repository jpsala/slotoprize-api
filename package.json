    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.8",
    "@types/jest": "^26.0.14",
    "@types/mysql2": "types/mysql2",
    "@types/node": "^14.11.1",
    "@types/source-map-support": "^0.5.3",
    "@typescript-eslint/eslint-plugin": "^4.1.1",
