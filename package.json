{
  "name": "api",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "test": "cross-env NODE_ENV=testing jest --runInBand --watch",
    "test2": "cross-env NODE_ENV=testing node --inspect=9230 node_modules/.bin/jest test/unit  --runInBand --watchAll",
    "test:debug": "node --inspect=9230 -r ts-node/register node_modules/.bin/jest --runInBand --watchAll",
    "build": "tsc",
    "start": "node build/index.js",
    "restart": "tsc && node build/index.js",
    "deploy-prod": "npm run build && ./deploy-prod.sh",
    "deploy-dev": "npm run build && ./deploy-dev.sh",
    "debug-remote": "concurrently \"tsc -w\" \"nodemon  --config nodemon.debug.remote.json build/index.js\"",
    "debug": "DEBUG=http concurrently \"tsc -w\" \"nodemon  --config nodemon.debug.json build/index.js\"",
    "dev": "concurrently \"tsc -w\" \"nodemon  --config nodemon.json build/index.js\"",
    "dev2": "cross-env NODE_ENV=development nodemon src/index.js --exec ts-node ",
    "pm2-dev": "pm2 start -n api npm -- run dev",
    "pm2-prod": "pm2 delete api ;npm run build && cross-env NODE_ENV=production pm2 start -n api npm -- run start",
    "pm2-restart": "pm2 delete api ; npm run pm2-prod"
  },
  "dependencies": {
    "@bevry/ansi": "^3.0.0",
    "@breejs/later": "^4.0.2",
    "@types/faker": "^5.1.0",
    "@types/http-errors": "^1.8.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/moment": "^2.13.0",
    "@types/node-schedule": "^1.3.0",
    "@types/pubsub-js": "^1.8.1",
    "@types/ws": "^7.2.6",
    "axios": "^0.21.0",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "camelcase-keys": "^6.2.2",
    "change-case": "^4.1.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "crypto": "^1.0.1",
    "date-fns": "^2.16.1",
    "express-async-errors": "^3.1.1",
    "express-formidable": "^1.2.0",
    "faker": "^5.1.0",
    "gmsmith": "^1.2.0",
    "http-errors": "^1.8.0",
    "http-status-codes": "^2.1.3",
    "inversify": "^5.0.1",
    "inversify-logger-middleware": "^3.1.0",
    "json-stringify-safe": "^5.0.1",
    "jsonwebtoken": "^8.5.1",
    "lodash.merge": "^4.6.2",
    "moment": "^2.28.0",
    "multer": "^1.4.2",
    "mysql2": "^1",
    "node-schedule": "^1.3.2",
    "nodemailer": "^6.4.14",
    "ololog": "^1.1.164",
    "pubsub-js": "^1.9.0",
    "querystring": "^0.2.0",
    "reflect-metadata": "^0.1.13",
    "snakecase-keys": "^3.2.0",
    "spritesmith": "^3.4.0",
    "string.bullet": "^1.0.12",
    "ws": "^7.3.1"
